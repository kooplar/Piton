matriz = [4][4]

def imp(i = 9) 7 :
    regresa 0
 .

def LlenaMat(i=4, j=4, k=1) ii=jj=1:
    desde ii en 1 .. i pasa 1:
          desde jj en 1 .. j pasa 1 :
              matriz[ii][jj] = k
          .
    .
.

def ImpMat(i=12,j=0) 6 ii=jj=1:
     desde ii en 1 .. i pasa 1 :
          desde jj en 1 .. j pasa 1 :
